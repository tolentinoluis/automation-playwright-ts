{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/e2e",
    "module": "commonjs",
    "target": "es2018",
    "types": [
      "node",
      "global",
      "playwright",
      "@types/chai",
      "@types/mocha"
    ],
    "paths": {
      "@config/*": [
        "./config/*"
      ],
      "@enums/*": [
        "./enums/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@type/*": [
        "./types/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
    }
  }
}
